{
    "name": "directo-apitests",
    "scripts": {
        "test": "jest --verbose",
        "test:cov": "jest --verbose --coverage",
        "lint": "eslint -c .eslintrc.js --ext .ts ./src",
        "lint:fix": "eslint -c .eslintrc.js --ext .ts --fix ./src"
    },
    "dependencies": {
        "dotenv": "8.2.0",
        "faker": "^5.1.0",
        "jsonwebtoken": "8.5.1",
        "node-fetch": "2.6.1",
        "ts-node": "^9.1.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/faker": "^5.1.5",
        "@types/jest": "^26.0.19",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.14.14",
        "@types/node-fetch": "^2.5.7",
        "@types/uuid": "^8.3.3",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "eslint": "^8.5.0",
        "eslint-plugin-jsdoc": "^37.4.0",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jest": "26.4.2",
        "ts-jest": "26.4.4",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "rootDir": "./",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        }
    }
}
