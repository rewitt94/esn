{
    "name": "directo-apitests",
    "scripts": {
        "test": "jest --verbose",
        "test:cov": "jest --verbose --coverage"
    },
    "dependencies": {
        "dotenv": "8.2.0",
        "jsonwebtoken": "8.5.1",
        "node-fetch": "2.6.1",
        "ts-node": "^9.1.1",
        "uuidv4": "^6.2.6"
    },
    "devDependencies": {
        "@types/jest": "^26.0.19",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/node": "^14.14.14",
        "@types/node-fetch": "^2.5.7",
        "jest": "26.4.2",
        "ts-jest": "26.4.4",
        "tslint": "^6.1.3",
        "typescript": "^4.0.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "ts"
        ],
        "rootDir": "./",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        }
    }
}
